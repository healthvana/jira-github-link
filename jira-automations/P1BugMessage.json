{
	"blocks": [
		{
			"type": "header",
			"text": {
				"type": "plain_text",
				"text": "{{issue.summary}}",
				"emoji": true
			}
		},
		{
			"type": "section",
			"text": {
				"type": "mrkdwn",
				"text": "*<{{issue.url}}|{{issue.key}}>* - Priority: {{issue.priority.name}}"
			}
		},
		{
			"type": "context",
			"elements": [
				{
					"type": "plain_text",
					"text": "<!here> A priority bug has been reported by: *{{issue.reporter.displayName}}*"
				}
			]
		},
		{
			"type": "divider"
		},
		{
			"type": "section",
			"fields": [
				{
					"type": "mrkdwn",
					"text": "*Is a critical system down?* {{issue.customfield_10845}}"
				}
			]
		},
				{
			"type": "section",
			"fields": [
				{
					"type": "mrkdwn",
					"text": "*Description:*\n{{issue.description}}"
				}
			]
		},
		{
			"type": "section",
			"fields": [
				{
					"type": "mrkdwn",
					"text": "*How many reports of the issue have been made by users?*\n{{issue.How many reports of the issue have been made by users?}}"
				},
				{
					"type": "mrkdwn",
					"text": "*Is this issue related to a single organization or clinic?*\n{{issue.customfield_10848}}"
				},
				{
					"type": "mrkdwn",
					"text": "*Customer:*\n{{issue.Customer}}"
				},
				{
					"type": "mrkdwn",
					"text": "*User Logins to use:*\n{{issue.User Logins to use}}"
				},
				{
					"type": "mrkdwn",
					"text": "*Is it End-User Facing?*\n{{issue.End User Facing}}"
				}
			]
		}
	]
}